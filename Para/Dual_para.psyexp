        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="next_flash_time = core.getTime() + stimulus_duration + random.normal(loc=between_stimuli, scale=1) + 1 # Initial flash time&amp;#10;&amp;#10;stimulus = &quot;X&quot;&amp;#10;all_letters = [&quot;M&quot;, &quot;N&quot;, &quot;P&quot;, &quot;L&quot;] &amp;#10;&amp;#10;last_flash_time = None &amp;#10;offset_time = core.getTime() + stimulus_duration&amp;#10;&amp;#10;# Set the mouse at the center of the screen&amp;#10;mouse.setPos([0,0])&amp;#10;&amp;#10;# Log the trial number&amp;#10;log.exp(f&quot;Trial begin: {trials.thisN+1}&quot;)&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
